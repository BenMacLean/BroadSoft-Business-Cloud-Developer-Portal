h1 Hub AppsList for {{storage.email}}
.row
    .col-sm-offset-2.col-sm-12(ng-repeat="app in userApps")
      h3.appContainer {{app.name}}
      button.btn.btn-primary.appContainer(ui-sref="internal.editApp({appName:app.name})") Edit
      button.btn.btn-primary.appContainer(ng-click="deleteApp(app.name)") Delete
.row
  .col-sm-offset-2.col-sm-6
    button.btn.btn-primary.form-control(ui-sref="internal.register") Create 
